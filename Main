function MtchDspFnc(MatchDisp) {MatchDisp=MatchDisp.join(";").replace(/([^;,]+),(\wZ F)/g, '$2,$1').replace(/([^;,]+),(\wB \w)/g, '$2,$1').replace(/([^;,]+),(\wZ M)/g, '$2,$1').replace(/([^;,]+),(\wA \w)/g, '$2,$1').replace(/([^;,]+),(Z\w F)/g, '$2,$1').replace(/([^;,]+),(B\w F)/g, '$2,$1').replace(/([^;,]+),(A\w F)/g, '$2,$1').replace(/([^;,]+),(\w\w M)/g, '$2,$1').replace(/([^;,]+),(B\w M)/g, '$2,$1').replace(/([^;,]+),(Z\w M)/g, '$2,$1').replace(/([^;,]+),(A\w M)/g, '$2,$1').replace(/AA M/g, '00$&').replace(/AZ M/g, '01$&').replace(/AB M/g, '02$&').replace(/AO M/g, '03$&').replace(/ZZ M/g, '04$&').replace(/ZB M/g, '05$&').replace(/ZO M/g, '06$&').replace(/BA M/g, '07$&').replace(/BZ M/g, '08$&').replace(/BB M/g, '09$&').replace(/BO M/g, '10$&').replace(/OZ M,/g, '11$&').replace(/OB M,/g, '12$&').replace(/OO M,/g, '13$&').replace(/AA F,/g, '12$&').replace(/AB F,/g, '13$&').replace(/AZ F,/g, '14$&').replace(/,OO F/g, '00$&').replace(/,OZ F/g, '01$&').replace(/,OB F/g, '03$&').replace(/,OA F/g, '04$&').replace(/,ZZ F/g, '05$&').replace(/,ZB F/g, '06$&').replace(/,ZA F/g, '07$&').replace(/,BO F/g, '08$&').replace(/,BZ F/g, '09$&').replace(/,BB F/g, '10$&').replace(/,BA F/g, '11$&').replace(/,AZ F/g, '12$&').replace(/,AB F/g, '13$&').replace(/,AA F/g, '14$&').replace(/,OO M/g, '15$&').replace(/,OB M/g, '16$&').replace(/,OZ M/g, '17$&').split(";").sort().join(";").replace(/\d+|&/g, '').replace(/,/g, ' + ').replace(/;/g, ' • '); return MatchDisp;}

function BinDspFnc(BinDisp) {BinDisp=BinDisp.toString().replace(/AA M/g, '00$&').replace(/AZ M/g, '01$&').replace(/AB M/g, '02$&').replace(/AO M/g, '03$&').replace(/ZZ M/g, '04$&').replace(/ZB M/g, '05$&').replace(/ZO M/g, '06$&').replace(/BA M/g, '07$&').replace(/BZ M/g, '08$&').replace(/BB M/g, '09$&').replace(/BO M/g, '10$&').replace(/OZ M,/g, '11$&').replace(/OB M,/g, '12$&').replace(/OO M,/g, '13$&').replace(/AA F,/g, '14$&').replace(/AB F,/g, '15$&').replace(/AZ F,/g, '16$&').replace(/BA F/g, '17$&').replace(/BB F/g, '18$&').replace(/BZ F/g, '19$&').replace(/BO F/g, '20$&').replace(/ZA F/g, '21$&').replace(/ZB F/g, '22$&').replace(/ZZ F/g, '23$&').replace(/OA F/g, '24$&').replace(/OB F/g, '25$&').replace(/OZ F/g, '26$&').replace(/OO F/g, '27$&').split(",").sort().toString().replace(/\d+|&/g, '').replace(/,/g, ' • '); return BinDisp;}

