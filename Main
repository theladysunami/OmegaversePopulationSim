function calcFrtF() {
FertTypeVal=document.getElementById("FertType").value;

mAfrt=Number(mAf.value) || 1;
mBfrt=Number(mBf.value) || 1;
mOfrt2=Number(mOf2.value) || 0;
mOfrt1=Number(mOf1.value) || 1;
fAfrt1=Number(fAf1.value) || 1;
fAfrt2=Number(fAf2.value) || 0;
fBfrt=Number(fBf.value) || 1;
fOfrt=Number(fOf.value) || 1;

mZfrt=(mAfrt+mBfrt)/2;
fZfrt=(fOfrt+fBfrt)/2
if (Zfrt.value=="FrFfthAFfthB")
{mZfrt=(4*mAfrt+mBfrt)/5;
fZfrt=(4*fOfrt+fBfrt)/5;}
if (Zfrt.value=="ThrFrthAFrthB")
{mZfrt=(3*mAfrt+mBfrt)/4;
fZfrt=(3*fOfrt+fBfrt)/4;}
if (Zfrt.value=="TwThrdAThrdB")
{mZfrt=(2*mAfrt+mBfrt)/3;
fZfrt=(2*fOfrt+fBfrt)/3;}
if (Zfrt.value=="ThrdATwThrdB")
{mZfrt=(mAfrt+2*mBfrt)/3;
fZfrt=(fOfrt+2*fBfrt)/3;}
if (Zfrt.value=="FrthAThrFrthB")
{mZfrt=(mAfrt+3*mBfrt)/4;
fZfrt=(fOfrt+3*fBfrt)/4;}
if (Zfrt.value=="FfthAFrFfthB")
{mZfrt=(mAfrt+4*mBfrt)/5;
fZfrt=(fOfrt+4*fBfrt)/5;}

if (FertTypeVal=="Add")
{aMoFfrt=Rnd100ths(mAfrt+fOfrt);
aMbFfrt=Rnd100ths(mAfrt+fBfrt);
aMaFfrt=Rnd100ths(mAfrt+fAfrt2);

bMoFfrt=Rnd100ths(mBfrt+fOfrt);
bMbFfrt=Rnd100ths(mBfrt+fBfrt);
bMaFfrt=Rnd100ths(mBfrt+fAfrt2);

oMoFfrt=Rnd100ths(mOfrt2+fOfrt);
oMbFfrt=Rnd100ths(mOfrt2+fBfrt);

oMaFfrt=Rnd100ths(mOfrt1+fAfrt1+mOfrt2+fAfrt2);

aFoFfrt=Rnd100ths(fAfrt1+fOfrt);
aFbFfrt=Rnd100ths(fAfrt1+fBfrt);
aFaFfrt=Rnd100ths(fAfrt1+fAfrt2);

aMoMfrt=Rnd100ths(mAfrt+mOfrt1);
bMoMfrt=Rnd100ths(mBfrt+mOfrt1);
oMoMfrt=Rnd100ths(mOfrt2+mOfrt1);

zMoFfrt=Rnd100ths(mZfrt+fOfrt);
zMbFfrt=Rnd100ths(mZfrt+fBfrt);
zMaFfrt=Rnd100ths(mZfrt+fAfrt2);
zMoMfrt=Rnd100ths(mZfrt+mOfrt1);

zMzFfrt=Rnd100ths(mZfrt+fZfrt);

aMzFfrt=Rnd100ths(mAfrt+fZfrt);
bMzFfrt=Rnd100ths(mBfrt+fZfrt);
oMzFfrt=Rnd100ths(mOfrt2+fZfrt);
aFzFfrt=Rnd100ths(fAfrt1+fZfrt);

if (mOfrt2==0)
{oMoFfrt=0; oMbFfrt=0; oMoMfrt=0; oMaFfrt=mOfrt1+fAfrt1;}
if (fAfrt2==0)
{aMaFfrt=0; bMaFfrt=0; aFaFfrt=0; oMaFfrt=mOfrt1+fAfrt1;}}

if (FertTypeVal=="Mult")
{aMoFfrt=Rnd100ths(mAfrt*fOfrt)
aMbFfrt=Rnd100ths(mAfrt*fBfrt)
aMaFfrt=Rnd100ths(mAfrt*fAfrt2)

bMoFfrt=Rnd100ths(mBfrt*fOfrt)
bMbFfrt=Rnd100ths(mBfrt*fBfrt)
bMaFfrt=Rnd100ths(mBfrt*fAfrt2)

oMoFfrt=Rnd100ths(mOfrt2*fOfrt)
oMbFfrt=Rnd100ths(mOfrt2*fBfrt)
oMaFfrt=Rnd100ths(mOfrt1*fAfrt1 +mOfrt2*fAfrt2)

aFoFfrt=Rnd100ths(fAfrt1*fOfrt)
aFbFfrt=Rnd100ths(fAfrt1*fBfrt)
aFaFfrt=Rnd100ths(fAfrt1*fAfrt2)

aMoMfrt=Rnd100ths(mAfrt*mOfrt1)
bMoMfrt=Rnd100ths(mBfrt*mOfrt1)
oMoMfrt=Rnd100ths(mOfrt2*mOfrt1)

zMoFfrt=Rnd100ths(mZfrt*fOfrt)
zMbFfrt=Rnd100ths(mZfrt*fBfrt)
zMaFfrt=Rnd100ths(mZfrt*fAfrt2)
zMoMfrt=Rnd100ths(mZfrt*mOfrt1)

zMzFfrt=Rnd100ths(mZfrt*fZfrt)

aMzFfrt=Rnd100ths(mAfrt*fZfrt)
bMzFfrt=Rnd100ths(mBfrt*fZfrt)
oMzFfrt=Rnd100ths(mOfrt2*fZfrt)
aFzFfrt=Rnd100ths(fAfrt1*fZfrt)}

IaMoFfrt=aMoFfrt; IaMbFfrt=aMbFfrt; IaMaFfrt=aMaFfrt;
IbMoFfrt=bMoFfrt; IbMbFfrt=bMbFfrt; IbMaFfrt=bMaFfrt;
IoMoFfrt=oMoFfrt; IoMbFfrt=oMbFfrt; IoMaFfrt=oMaFfrt;
IaFoFfrt=aFoFfrt; IaFbFfrt=aFbFfrt; IaFaFfrt=aFaFfrt;
IaMoMfrt=aMoMfrt; IbMoMfrt=bMoMfrt; IoMoMfrt=oMoMfrt;

if (CmptTst.value=="Cstm") {
var FrtCtmClss=document.getElementsByClassName('Frt')
for (var i=0; i < FrtCtmClss.length; i++) {FrtCtmClss[i].removeAttribute('hidden');}

if (mAfOfCI.checked) {aMoFfrt=IaMoFfrt*1.05;} else if (mAfOfCD.checked) {aMoFfrt=IaMoFfrt*0.975;}
if (mAfBfCI.checked) {aMbFfrt=IaMbFfrt*1.025;} else if (mAfBfCD.checked) {aMbFfrt=IaMbFfrt*0.95;}
if (mAfAfCI.checked) {aMaFfrt=IaMaFfrt*1.025;} else if (mAfAfCD.checked) {aMaFfrt=IaMaFfrt*0.9;}
if (mAmOfCI.checked) {aMoMfrt=IaMoMfrt*1.05;} else if (mAmOfCD.checked) {aMoMfrt=IaMoMfrt*0.975;}
if (mAfZfCI.checked) {aMzFfrt=aMzFfrt*1.05;} else if (mAfZfCD.checked) {aMzFfrt=aMzFfrt*0.975;}

if (mBfOfCI.checked) {bMoFfrt=IbMoFfrt*1.025;} else if (mBfOfCD.checked) {bMoFfrt=IbMoFfrt*0.95;}
if (mBfBfCI.checked) {bMbFfrt=IbMbFfrt*1.05;} else if (mBfBfCD.checked) {bMbFfrt=IbMbFfrt*0.975;}
if (mBfAfCI.checked) {bMaFfrt=IbMaFfrt*1.025;} else if (mBfAfCD.checked) {bMaFfrt=IbMaFfrt*0.95;}
if (mBmOfCI.checked) {bMoMfrt=IbMoMfrt*1.025;} else if (mBmOfCD.checked) {bMoMfrt=IbMoMfrt*0.95;}
if (mBfZfCI.checked) {bMzFfrt=bMzFfrt*1.025;} else if (mBfZfCD.checked) {bMzFfrt=bMzFfrt*0.95;}

if (mOfOfCI.checked) {oMoFfrt=IoMoFfrt*1.025;} else if (mOfOfCD.checked) {oMoFfrt=IoMoFfrt*0.9;}
if (mOfBfCI.checked) {oMbFfrt=IoMbFfrt*1.025;} else if (mOfBfCD.checked) {oMbFfrt=IoMbFfrt*0.95;}
if (mOfAfCI.checked) {oMaFfrt=IoMaFfrt*1.05;} else if (mOfAfCD.checked) {oMaFfrt=IoMaFfrt*0.975;}
if (mOmOfCI.checked) {oMoMfrt=IoMoMfrt*1.025;} else if (mOmOfCD.checked) {IoMoMfrt=oMoMfrt*0.9;}
