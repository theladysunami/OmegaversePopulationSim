// Replication "Errors"
function ErrAdjVal(RtoVal,NmbrVal) {NewVal=Rnd1000ths(Math.min(RtoVal,1)*(NmbrVal/100)); return NewVal;}
abMaaM=ErrAdjVal(IAllB/IAllAnO,abMaaMe.value) || 0;
obFooF=ErrAdjVal(IAllB/IAllAnO,obFooFe.value) || 0;
abMbbM=ErrAdjVal(IAllAnO/IAllB,abMbbMe.value) || 0;
obFbbF=ErrAdjVal(IAllAnO/IAllB,obFbbFe.value) || 0;
azMaaM=ErrAdjVal(IAllZ/IAllAnO,azMaaMe.value) || 0;
ozFooF=ErrAdjVal(IAllZ/IAllAnO,ozFooFe.value) || 0;
zbMbbM=ErrAdjVal(IAllZ/IAllB,zbMbbMe.value) || 0;
zbFbbF=ErrAdjVal(IAllZ/IAllB,zbFbbFe.value) || 0;

